    "@types/webpack-merge": "5.0.0",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "cross-env": "7.0.3",
    "eslint": "7.25.0",
    "html-webpack-plugin": "5.3.1",
    "lint-staged": "10.5.4",
    "source-map-loader": "2.0.1",
    "ts-loader": "9.1.2",
